{
  "extends": ["prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "spaced-comment": "off",
    "no-console": "error",
    "consistent-return": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val" }]
  }
}
